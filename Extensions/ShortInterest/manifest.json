{
    "name": "ShortInterest",
    "version": "1.0", 
    "background": {
        "scripts": [
          "lib/jquery.min.js", 
          "lib/lib.js",
          "util/retrievers.js", 
          "sites/finra_daily_report.js", 
          "sites/finra_next_report_date.js", 
          "sites/fintel.js", 
          "sites/iborrow.js", 
          "sites/marketbeat.js", 
          "sites/etf_indirect_shorting.js",
          "background.js"],
        "persistent": false
      },
    "description": "Consolidates a number of sources onto a single page",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "bookmarks",
        "storage",
        "tabs",
        "http://regsho.finra.org/*",
        "https://fintel.io/ss/us/*",
        "https://www.marketbeat.com/stocks/NYSE/*",
        "https://iborrowdesk.com/report/*",
        "https://www.finra.org/filing-reporting/regulatory-filing-systems/short-interest#overview"
      ],
      "omnibox": { "keyword" : "si" }
  }