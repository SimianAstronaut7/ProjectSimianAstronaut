{
    "name": "Quarterly Checks",
    "version": "1.0",
    "icons": { 
      "16": "img/quarter16.png",
      "48": "img/quarter48.png",
      "128": "img/quarter128.png" 
    },
    "background": {
        "scripts": [
          "lib/constants.js",
          "lib/lib.js",
          "background.js"],
        "persistent": false
      },
    "description": "Opens the research that should be done on a quarterly basis. TODO: Look at adding an alarm/alert to this. ",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "bookmarks",
        "storage",
        "tabs"
      ],
      "omnibox": { "keyword" : "q" }
  }