{
    "name": "MeasuringHype",
    "version": "1.0", 
    "background": {
        "scripts": ["jquery-3.5.1.min.js", "background.js"],
        "persistent": false
      },
    "description": "Opens a number of resources to measure how much people are looking for the ticker.",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "https://finance.yahoo.com/",
        "https://swaggystocks.com/dashboard/wallstreetbets/ticker-sentiment/*"
      ],
      "options_page": "options.html",
      "page_action": {
        "default_popup": "popup.html"
      },
      "omnibox": { "keyword" : "hype" }
    
  }