{
    "name": "MeasuringHype",
    "version": "1.0",
    "icons": { 
      "16": "img/happy16.png",
      "48": "img/happy48.png",
     "128": "img/happy128.png" 
    },
    "background": {
        "scripts": [
          "lib/jquery.min.js",
          "lib/constants.js",
          "lib/lib.js",
          "background.js"],
        "persistent": false
      },
    "description": "Opens a number of resources to measure how much people are looking for the ticker.",
    "manifest_version": 2,
    "permissions": [
      "activeTab",
      "bookmarks",
        "storage",
        "tabs",
        "https://finance.yahoo.com/",
        "https://swaggystocks.com/dashboard/wallstreetbets/ticker-sentiment/*"
      ],
      "options_page": "options.html",
      "page_action": {
        "default_popup": "popup.html"
      },
      "omnibox": { "keyword" : "hype" }
    
  }